<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<views xmlns="http://schemas.haulmont.com/cuba/view.xsd">

    <view entity="fintech_DebitCard" name="debitCard-view" extends="_minimal">
        <property name="middleName"/>
        <property name="lastName"/>
        <property name="birthdate"/>
        <property name="phoneNumber"/>
        <property name="email"/>
        <property name="address"/>
        <property name="passportNumber"/>
        <property name="passportSerial"/>
        <property name="passportDate"/>
        <property name="passportOrganization"/>
        <property name="status"/>
    </view>
    <view entity="fintech_TelegramUser" name="telegramUser-chat-view" extends="_local">
        <property name="messages" view="_minimal">
            <property name="text"/>
            <property name="cubaUser" view="_minimal"/>
            <property name="datetime"/>
        </property>
    </view>
    <view entity="fintech_Message" name="message-chat-view" extends="_local">
        <property name="cubaUser" view="_minimal"/>
        <property name="telegramUser" view="_minimal">
            <property name="telegramId"/>
        </property>
    </view>
</views>